{
  "name": "Ps cypress",
  "id": "com.adobe.photoshop.cypress",
  "version": "0.1.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "23.5.0",
    "data" : {
      "apiVersion": 2
    }
  },
  "manifestVersion": 4,
  "entrypoints": [
    {
      "type": "panel",
      "id": "cypress",
      "label": {
        "default": "cypress ðŸŒ²"
      },
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark-panel.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ],
          "species": [
            "chrome"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light-panel.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "chrome"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}
